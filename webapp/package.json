{
  "name": "kyc-dmn-webapp",
  "version": "0.0.1",
  "description": "An open source build scaffolding utility for web apps.",
  "main": "server.js",
  "repository": "https://github.com/patternfly/patternfly-react-seed.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "webpack --config webpack.prod.js && npm run dr:surge",
    "start": "node server.js",
    "start:dev": "webpack-dev-server --hot --color --progress --config webpack.dev.js",
    "test": "jest",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts}",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^8.6.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.3.0",
    "prettier": "^2.4.1",
    "serve": "^13.0.2",
    "ts-loader": "^9.2.5",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.2.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@patternfly/react-core": "^4.168.8",
    "@patternfly/react-icons": "^4.19.8",
    "@patternfly/react-styles": "^4.18.8",
    "@react-keycloak/web": "^3.4.0",
    "@storybook/builder-webpack5": "^6.5.13",
    "file-loader": "^6.2.0",
    "keycloak-js": "^16.1.0",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-axe": "^3.5.4",
    "react-dom": "^17.0.2",
    "react-router-last-location": "^2.0.1",
    "style-loader": "^3.2.1",
    "ts-config": "^20.5.0",
    "url-loader": "^4.1.1",
    "whatwg-fetch": "^3.6.2"
  }
}
